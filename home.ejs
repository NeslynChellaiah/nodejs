<%- include('Partials/header') -%>
<h1>Home</h1>
<p>
    <%= homeStartingContent %>
</p>
    <% posts.forEach(function(post){ %>
        <h1>
            <%= post.contentTitle %>
        </h1>
        <p>
            <%= post.content.slice(0,100) + " ..." %>
        </p>
        <a href="/posts/<%= post.contentTitle %>">Read more</a>
    <% }); %>
<%- include('Partials/footer') -%>